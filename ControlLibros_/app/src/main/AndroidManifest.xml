<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.controllibros_">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/baseline_camera_alt_black_48"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/baseline_camera_alt_black_48"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".BienvenidaActivity"
            android:label="@string/title_activity_bienvenida"
            android:theme="@style/AppBienvenida">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" android:windowSoftInputMode="stateVisible">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>